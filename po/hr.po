msgstr "<b>_Sustavsko kodiranje za unos</b>"

#: gnucash/gtkbuilder/assistant-xml-encoding.glade:345
msgid "<b>_Custom encoding</b>"
msgstr "<b>_Proizvoljno kodiranje</b>"

