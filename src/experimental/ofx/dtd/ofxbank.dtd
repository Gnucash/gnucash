<!-- Payment instructions for a wire transfer -->
<!ELEMENT PAYINSTRUCT	- o	%STRTYPE>

<!-- Fee assessed for the transfer -->
<!ELEMENT FEE	- o	%AMTTYPE>

<!-- Description of fee -->
<!ELEMENT FEEMSG    - o %STRTYPE>

<!-- Transfer confirmation message -->
<!ELEMENT CONFMSG	- o	%STRTYPE>







