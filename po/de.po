"HBCI\n"
"Version"
msgstr ""
"HBCI-\n"
"Version"

#: src/import-export/hbci/glade/hbci.glade:1212
msgid ""
"Update \n"
"account list"
msgstr ""
"Kontoliste\n"
"aktualisieren"

#: src/import-export/hbci/glade/hbci.glade:1226
msgid ""
"Manually add\n"
"Account"
msgstr ""
"Manuell Konto\n"
"hinzufügen"

#: src/import-export/hbci/glade/hbci.glade:1240
msgid ""
"Add \n"
"Bank"
